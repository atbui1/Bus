        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBtlfSbj3Oj8vLObfCUIlrWyE_OiLknV8w"
        }
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION", 
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
